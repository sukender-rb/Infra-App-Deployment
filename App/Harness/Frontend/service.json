{
  "desiredCount": <+pipeline.variables.DesiredCount>,
  "launchType": "FARGATE",
  "serviceName": "<+pipeline.variables.containername>",
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 100,
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    }
  },
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "<+pipeline.variables.SubnetA>",
        "<+pipeline.variables.SubnetB>",
        "<+pipeline.variables.SubnetC>"
      ],
      "securityGroups": [
        "<+pipeline.variables.SecurityGroup>"
      ],
      "assignPublicIp": "ENABLED"
    }
  },
  "loadBalancers": [
    {
      "targetGroupArn": "<+pipeline.variables.targetGroupArn>",
      "containerName": "<+pipeline.variables.containername>",
      "containerPort": <+pipeline.variables.containerport>
    }
  ]
}
